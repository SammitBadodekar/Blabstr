generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  id String @id @default(uuid())
  name String
  imageUrl String @db.Text
  email String @db.Text
  password String? 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

